# Node runtime
NODE_ENV=development

# Backend
BACKEND_PORT=3001
BACKEND_API_URL=http://localhost:3001
BACKEND_INTERNAL_API_KEY=replace-with-secure-value
EXTENSION_ORIGIN=chrome-extension://replace-with-extension-id
VISUALIZER_ORIGIN=https://replace-with-visualizer-origin
RATE_LIMIT_PER_MINUTE=60
ANALYZE_CACHE_TTL_MS=300000

# Extension <-> Visualization Bridge (Vite requires VITE_ prefix for client access)
VITE_EXTENSION_ORIGIN=chrome-extension://replace-with-extension-id
VITE_VISUALIZER_URL=https://replace-with-visualizer-url

# Supabase
SUPABASE_ACCESS_TOKEN=replace-with-supabase-access-token
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=replace-with-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=replace-with-supabase-service-role-key
SUPABASE_JWT_SECRET=replace-with-supabase-jwt-secret

# Observability (Sentry â€” leave blank to disable)
SENTRY_DSN=
VITE_SENTRY_DSN=

# Internal analysis model (project-owned)
GEMINI_API_KEY=replace-with-gemini-api-key

# User-provided provider defaults for local development only
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
PERPLEXITY_API_KEY=

# Test/e2e environment
RUN_EXTENSION_BROWSER_E2E=0
EXTENSION_UNPACKED_PATH=
CI=
